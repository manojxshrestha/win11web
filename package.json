{
  "name": "windows-11-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter windows-11-web dev",
    "dev-debug": "pnpm --filter windows-11-web dev-debug",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "test:component": "turbo run test:component",
    "test:component:headless": "turbo run test:component:headless",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-packagejson": "^2.4.12",
    "turbo": "^1.13.0"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
